{
  "display_information": {
      "name": "atlasBot",
      "description": "Slack bot for Atlassian",
      "background_color": "#614b79"
  },
  "features": {
      "bot_user": {
          "display_name": "atlasBot",
          "always_online": true
      }
  },
  "oauth_config": {
      "redirect_urls": [
          "https://atlasbot.slackapps.example.com/0_0_1/slack-oauth-redirect",
          "https://id.atlassian.com/outboundAuth/finish"
      ],
      "scopes": {
          "user": [
              "chat:write"
          ],
          "bot": [
              "chat:write",
              "users:read.email",
              "users:read"
          ]
      }
  },
  "settings": {
      "org_deploy_enabled": true,
      "socket_mode_enabled": false,
      "token_rotation_enabled": false
  }
}